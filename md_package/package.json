{
  "name": "@jupyterlab/example-federated-md",
  "version": "1.0.0",
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "jupyter labextension build .",
    "build:lib": "tsc",
    "build:all": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf md_package/static",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "install-ext": "pip install -e . && jupyter labextension develop .",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w",
    "watch:labextension": "jupyter labextension watch .",
    "watch:all": "run-p watch watch:labextension"
  },
  "main": "lib/index.js",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-alpha.11",
    "@jupyterlab/apputils": "^3.0.0-alpha.11",
    "@jupyterlab/coreutils": "^5.0.0-alpha.11",
    "@jupyterlab/example-federated-middle": "~2.1.0",
    "@jupyterlab/markdownviewer": "^3.0.0-alpha.11",
    "@jupyterlab/rendermime": "^3.0.0-alpha.11",
    "@jupyterlab/settingregistry": "^3.0.0-alpha.11"
  },
  "devDependencies": {
    "@jupyterlab/buildutils": "~3.0.0-alpha.11",
    "@types/react": "16.9.41",
    "npm-run-all": "^4.1.5",
    "rimraf": "~3.0.0",
    "typedoc": "^0.17.7",
    "typescript": "~3.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "md_package/static",
    "singletonPackages": [
      "@jupyterlab/example-federated-middle"
    ]
  }
}
