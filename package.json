{
  "name": "@jupyterlab/example-federated",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:core && npm run build:json_package && npm run build:middle && npm run build:md && npm run build:theme",
    "build:core": "cd core_package && npm run build",
    "build:json": "node build-ext.js ./json_package",
    "build:middle": "node build-ext.js ./middle_package",
    "build:md": "node build-ext.js ./md_package",
    "build:theme": "node build-ext.js ./theme_package",
    "watch:md": "node build-ext.js ./md_package --watch",
    "clean": "rimraf labextensions"
  },
  "devDependencies": {
    "ajv": "^6.12.3",
    "rimraf": "^3.0.2",
    "webpack": "5.0.0-beta.22",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^5.0.9"
  },
  "workspaces": [
    "*"
  ],
  "dependencies": {
    "commander": "^5.1.0"
  }
}
