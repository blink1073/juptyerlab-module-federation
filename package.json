{
  "name": "@jupyterlab/example-federated",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:core && npm run build:json && npm run build:middle && npm run build:md && npm run build:theme",
    "build:core": "cd core_package && npm run build",
    "build:json": "build-labextension --core-path ./core_package ./json_package",
    "build:middle": "build-labextension --core-path ./core_package ./middle_package",
    "build:md": "build-labextension --core-path ./core_package ./md_package",
    "build:theme": "build-labextension --core-path ./core_package ./theme_package",
    "build:core:prod": "cd core_package && npm run build:prod",
    "build:json:prod": "build-labextension --core-path ./core_package --prod ./json_package",
    "build:prod": "npm run build:core:prod && npm run build:json:prod",
    "watch:md": "build-labextension --core-path ./core_package ./md_package --watch"
  },
  "devDependencies": {
    "@jupyterlab/buildutils": "~3.0.0-alpha.6",
    "ajv": "^6.12.3",
    "rimraf": "^3.0.2",
    "webpack": "5.0.0-beta.21",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^5.0.9"
  },
  "workspaces": [
    "*"
  ],
  "dependencies": {
    "commander": "^5.1.0"
  }
}
